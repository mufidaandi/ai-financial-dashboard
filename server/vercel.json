{
  "version": 2,
  "builds": [
    {
      "src": "src/simple-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/test.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/test",
      "dest": "api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/simple-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}